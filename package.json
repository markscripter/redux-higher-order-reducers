{
    "name": "@wecreatesoftware/redux-higher-order-reducers",
    "version": "2.0.0",
    "description": "Generic higher order reducers.",
    "main": "lib/wecreate/software/redux.higher.order.reducers.js",
    "module": "esm/wecreate/software/redux.higher.order.reducers.js",
    "scripts": {
        "build": "npm run build:esm && npm run build:commonjs && npm run build:umd",
        "healthcheck": "npm run lint && npm run test",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
        "build:esm": "BABEL_ENV=esm babel src --out-dir esm --ignore '**/*spec.js'",
        "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib --ignore '**/*spec.js'",
        "build:umd": "webpack --mode production --config webpack.config.babel.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wecreatesoftware/redux-higher-order-reducers"
    },
    "keywords": [
        "react",
        "redux",
        "react-redux",
        "reducers",
        "higher order reducers",
        "reuseable reducers"
    ],
    "author": "Matt Struensee <matt@wecreate.software>",
    "license": "MIT",
    "devDependencies": {
        "@wecreatesoftware/eslint-config": "1.2.9",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.3",
        "babel-jest": "23.6.0",
        "babel-loader": "7.1.5",
        "babel-plugin-transform-object-rest-spread": "6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-react": "6.24.1",
        "babel-register": "6.26.0",
        "coveralls": "3.0.2",
        "jest": "23.6.0",
        "parallel-webpack": "2.3.0",
        "react": "16.7.0",
        "react-dom": "16.7.0",
        "react-redux": "6.0.0",
        "redux": "4.0.1",
        "webpack": "4.27.1",
        "webpack-cli": "3.1.2"
    }
}
